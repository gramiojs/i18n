{
	"name": "@gramio/i18n",
	"version": "1.0.0",
	"description": "i18n plugin for GramIO with type-safety",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./fluent": {
			"import": {
				"types": "./dist/fluent/index.d.ts",
				"default": "./dist/fluent/index.js"
			},
			"require": {
				"types": "./dist/fluent/index.d.cts",
				"default": "./dist/fluent/index.cjs"
			}
		}
	},
	"type": "module",
	"homepage": "https://github.com/gramiojs/i18n",
	"readme": "https://gramio.dev/plugins/official/i18n.html",
	"scripts": {
		"prepublishOnly": "bunx pkgroll",
		"generate": "bun scripts/generate.ts"
	},
	"license": "MIT",
	"keywords": [
		"gramio",
		"gramio-plugin",
		"i18n",
		"internationalization",
		"fluent",
		"i10n",
		"locales",
		"languages",
		"localization",
		"translation",
		"telegram-bot-api",
		"telegram-bot",
		"i18n-in-ts"
	],
	"devDependencies": {
		"@biomejs/biome": "1.9.3",
		"@types/bun": "^1.1.10",
		"gramio": "^0.0.50",
		"pkgroll": "2.5.0",
		"prettier": "^3.3.3",
		"typescript": "^5.6.2"
	},
	"peerDependencies": {
		"gramio": ">=0.0.39 <1.0.0",
		"@fluent/bundle": "^0.18.0"
	},
	"files": ["dist"],
	"peerDependenciesMeta": {
		"@fluent/bundle": {
			"optional": true
		}
	}
}
